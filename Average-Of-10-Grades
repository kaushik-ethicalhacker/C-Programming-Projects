#include <stdio.h>

int main(void)
{
    int grades[10];
    int count = 10;
    long sum = 0;
    float avg =0.0;
    
    printf("Enter the 10 grades:\n");
    
    for(int i = 0; i< count;++i){
        printf("%2u)",i+1);
        scanf("%d",&grades[i]);
        sum += grades[i];
    }
    avg = (float)sum/count;
    printf("\nAverage of 10 grades entered is %.2f\n",avg);
    
    return 0;
}